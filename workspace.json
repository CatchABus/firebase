{
  "version": 2,
  "projects": {
    "demo": {
      "root": "apps/demo/",
      "sourceRoot": "apps/demo/src",
      "projectType": "application",
      "prefix": "demo",
      "targets": {
        "build": {
          "executor": "@nativescript/nx:build",
          "options": {
            "noHmr": true,
            "production": true,
            "uglify": true,
            "release": true,
            "forDevice": true
          }
        },
        "ios": {
          "executor": "@nativescript/nx:build",
          "options": {
            "platform": "ios",
            "noHmr": true
          }
        },
        "android": {
          "executor": "@nativescript/nx:build",
          "options": {
            "platform": "android",
            "noHmr": true
          }
        },
        "clean": {
          "executor": "@nativescript/nx:build",
          "options": {
            "clean": true
          }
        }
      }
    },
    "demo-angular": {
